# Path to your oh-my-zsh installation.
export ZSH=~/.oh-my-zsh

# Path to pwendok
export PWENDOK=~/.pwendok

# Configure Cask destination folder
export HOMEBREW_CASK_OPTS="--appdir=/Applications"

# Configuration
    # Theme
ZSH_THEME="neoleny"
    # Custom folder location
ZSH_CUSTOM=$PWENDOK/zsh/custom
    # Plugins
plugins=(git brew brew-cask depo docker docker-compose encode64 golang grunt gulp k leny-git-prompt npm pip python sote zsh-syntax-highlighting)

source $ZSH/oh-my-zsh.sh
