# Path to your oh-my-zsh installation.
export ZSH=~/.oh-my-zsh

# Path to pwendok
export PWENDOK=~/.pwendok

# nvm configuration
export NVM_DIR=~/.nvm
. $(brew --prefix nvm)/nvm.sh

# Configuration
    # Theme
ZSH_THEME="leny"
    # Custom folder location
ZSH_CUSTOM=$PWENDOK/zsh/custom
    # Plugins
plugins=(git brew depo docker sote docker-compose leny-git-prompt)

source $ZSH/oh-my-zsh.sh
